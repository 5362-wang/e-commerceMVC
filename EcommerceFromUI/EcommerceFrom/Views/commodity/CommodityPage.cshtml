<script>

    //类型绑定
    $().ready({
        $.ajax({
            url: "/Commodity/CommodityBindType",
            type: "post",
            dataType: "json",
            success: function (res) {
                $(res).each(function () {
                    var sp = "<option>" + this.TypeName + "</option>";
                    $("#Select1").append(sp);
                })
            }
        })
    })

    //查询
    function Toquery(page) {
        $.ajax({
            type: "POST",
            url: '/Commodity/ComodityInquire',
            data: { pageNum: 1, pageSize: 10 },
            success: function (data) {
                HideLoad('body');
                $("#result").html(data);
            },
            error: function (xhr) {
                HideLoad('body');
                displayError(xhr);
            }
        });
        if (page = "add") {
            var insert = $("#insert");
            insert.css("display", "none");
        }
        if (page = "update") {
            var modify = $("#modify")
            modify.css("display", "none");
        }
        var query = $("#search");
        query.css("display", "block");
        var result = $("#result");
        result.css("display", "block");

    }


</script>






